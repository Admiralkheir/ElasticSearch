<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <!--key ve value olmak üzere iki farklı attribute bulunur. Bu sayede kod satırında değişiklik yapıp tekrar build etmeden burdan değişiklik yaparak güncellenebilir.-->
    <add key="ElasticSearchURI" value="http://localhost:9200"/>
    <add key="ElasticSearchIndexName" value="product_search"/>
  </appSettings>
</configuration>